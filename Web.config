<configuration>

  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
    
    <sessionState timeout="120"></sessionState>

    <customErrors mode="On" defaultRedirect="Fejl.aspx">
      <error statusCode="404" redirect="404.aspx"/>
    </customErrors>
  </system.web>

  <connectionStrings>
    <add name="MyCon" connectionString="Data Source=(local);Initial Catalog=Plakaten;Integrated Security=True"/>
  </connectionStrings>

  <appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None" />
  </appSettings>



</configuration>
